@prefix core: <http://w3id.org/polifonia/ontology/core/> .
@prefix jams: <http://w3id.org/polifonia/ontology/jams/> .
@prefix mc: <http://w3id.org/polifonia/ontology/musical-composition/> .
@prefix mf: <http://w3id.org/polifonia/ontology/musical-features/> .
@prefix pon-resource: <http://w3id.org/polifonia/resource/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

jams:foo a jams:JAMSFile ;
    jams:identifiers [ ] ;
    jams:jamsVersion "0.3.4" ;
    jams:release "" ;
    prov:wasDerivedFrom "when-in-rome" ;
    prov:wasMemberOf <https://github.com/jonnybluesman/choco> .

pon-resource:Agent%2Fannotator_local_id a core:Agent ;
    rdfs:label "Anne Otator" ;
    jams:hasAnnotatorType pon-resource:annotator-expert-human ;
    jams:hasName "Anne Otator" .

<http://w3id.org/polifonia/resource/Agent/Composer/John_Lennon> a core:Agent ;
    rdfs:label "John Lennon" .

<http://w3id.org/polifonia/resource/Agent/Composer/Paul_McCartney> a core:Agent ;
    rdfs:label "Paul McCartney" .

pon-resource:Score%2Fscore_local_id a mf:Score ;
    rdfs:label "Michelle" ;
    mc:isScoreOf pon-resource:MusicalComposition%2Fsong_local_id .

<http://w3id.org/polifonia/resource/JAMSAnnotation/score> a jams:JAMSAnnotation,
        jams:JAMSScoreAnnotation ;
    jams:annotationValidFrom <http://w3id.org/polifonia/resource/JAMSMetricalTime/1> ;
    jams:hasValidityDuration "45.0"^^xsd:float ;
    jams:includesObservation <http://w3id.org/polifonia/resource/JAMSObservation/A:min@1.3>,
        <http://w3id.org/polifonia/resource/JAMSObservation/A@1.1>,
        <http://w3id.org/polifonia/resource/JAMSObservation/E@1.1> .

<http://w3id.org/polifonia/resource/JAMSMetricalTime/1> jams:MetricalTime jams:MetricalTime ;
    jams:hasBeat "0"^^xsd:int ;
    jams:hasMeasure "1"^^xsd:int ;
    jams:isMetricalTimeOf <http://w3id.org/polifonia/resource/JAMSAnnotation/score> .

<http://w3id.org/polifonia/resource/JAMSMetricalTime/A:min@1.3> jams:MetricalTime jams:MetricalTime ;
    jams:isMetricalTimeOf <http://w3id.org/polifonia/resource/JAMSObservation/A:min@1.3> .

<http://w3id.org/polifonia/resource/JAMSMetricalTime/A@1.1> jams:MetricalTime jams:MetricalTime ;
    jams:isMetricalTimeOf <http://w3id.org/polifonia/resource/JAMSObservation/A@1.1> .

<http://w3id.org/polifonia/resource/JAMSMetricalTime/E@1.1> jams:MetricalTime jams:MetricalTime ;
    jams:isMetricalTimeOf <http://w3id.org/polifonia/resource/JAMSObservation/E@1.1> .

<http://w3id.org/polifonia/resource/JAMSObservation/A:min@1.3> a jams:JAMSObservation,
        jams:JAMSScoreObservation,
        jams:ScoreChordOccurrence ;
    rdfs:label "A:min" ;
    jams:hasDuration "2.0"^^xsd:float ;
    jams:ofChord <http://purl.org/ontology/chord/resource/A:min> ;
    mf:startsAt <http://w3id.org/polifonia/resource/JAMSMetricalTime/A:min@1.3> .

<http://w3id.org/polifonia/resource/JAMSObservation/A@1.1> a jams:JAMSObservation,
        jams:JAMSScoreObservation,
        jams:ScoreKeyModeOccurrence ;
    rdfs:label "A" ;
    jams:hasDuration "45.0"^^xsd:float ;
    jams:ofChord <http://purl.org/ontology/chord/resource/A> ;
    mf:startsAt <http://w3id.org/polifonia/resource/JAMSMetricalTime/A@1.1> .

<http://w3id.org/polifonia/resource/JAMSObservation/E@1.1> a jams:JAMSObservation,
        jams:JAMSScoreObservation,
        jams:ScoreChordOccurrence ;
    rdfs:label "E" ;
    jams:hasDuration "2.0"^^xsd:float ;
    jams:ofChord <http://purl.org/ontology/chord/resource/E> ;
    mf:startsAt <http://w3id.org/polifonia/resource/JAMSMetricalTime/E@1.1> .

